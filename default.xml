<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="poky"  fetch="git://git.yoctoproject.org/poky"/>
    <remote name="oe" fetch="https://git.openembedded.org/meta-openembedded"/>
    <remote name="raspi"    fetch="git://git.yoctoproject.org/meta-raspberrypi"/>

    <default sync-j="20"/>

    <project remote="poky"  revision="origin/kirkstone"  name="poky" path=""/>
    <project remote="oe"    revision="origin/kirkstone"  name="meta-openembedded" path="poky"/>
    <project remote="raspi" revision="origin/kirkstone"  name="meta-raspberrypi" path="poky"/>

</manifest>
