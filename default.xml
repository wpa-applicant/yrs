<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <remote name="poky"                   fetch="git://git.yoctoproject.org"/>
    <remote name="meta-openembedded"      fetch="https://git.openembedded.org"/>
    <remote name="meta-raspberrypi"       fetch="git://git.yoctoproject.org"/>

    <default sync-j="20"/>

    <project remote="poky"                revision="kirkstone"   name="poky"                path="poky"/>
    <project remote="meta-openembedded"   revision="kirkstone"   name="meta-openembedded"   path="poky/meta-openembedded"/>
    <project remote="meta-raspberrypi"    revision="kirkstone"   name="meta-raspberrypi"    path="poky/meta-raspberrypi"/>

</manifest>
